-- ----------------------------
-- Table structure for mobile
-- ----------------------------
DROP TABLE IF EXISTS `mobile`;
CREATE TABLE `mobile` (
  `id` int(11) NOT NULL AUTO_INCREMENT COMMENT 'id',
  `category_id` int(11) DEFAULT 100042 COMMENT '分类id',
  `name` varchar(100) NOT NULL COMMENT '名称',
  `subtitle` varchar(200) DEFAULT NULL COMMENT '副标题',
  `main_image` varchar(500) DEFAULT NULL COMMENT '主图地址',
  `sub_images` text DEFAULT NULL COMMENT '图片地址',
  `detail` text DEFAULT NULL COMMENT '详情',
  `price` decimal(20,2) NOT NULL COMMENT '价格',
  `stock` int(11) NOT NULL COMMENT '库存信息',
  `status` int(6) DEFAULT 1 COMMENT '状态 1-在售 2-下架 3-删除',
  `create_time` datetime DEFAULT NULL COMMENT '创建时间',
  `update_time` datetime DEFAULT NULL COMMENT '修改时间',
  `sold` int(11) DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=40333 DEFAULT CHARSET=utf8;

